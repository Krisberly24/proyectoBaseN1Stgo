<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_background"
    tools:context=".Casa_act">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView4"
        android:layout_width="340dp"
        android:layout_height="324dp"
        android:layout_marginTop="40dp"
        app:cardCornerRadius="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="297dp"
                android:layout_height="55dp"
                android:layout_marginTop="16dp"
                android:text="Bienvenido a HealthySoul"
                android:textColor="#b00122"
                android:textSize="24sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="252dp"
                android:layout_height="113dp"
                android:layout_marginTop="4dp"
                android:text="HealthySoul es la aplicación que te ayudara a lograr  tus objetivos, una alimentación que perdurara en el tiempo sin restricciones solo con una alimentación adecuada y saludable"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView6" />

            <VideoView
                android:id="@+id/vwgym"
                android:layout_width="278dp"
                android:layout_height="117dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView5"
        android:layout_width="353dp"
        android:layout_height="85dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        app:cardCornerRadius="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView4">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="339dp"
            android:layout_height="match_parent">

            <ImageButton
                android:id="@+id/imageButton6"
                android:layout_width="89dp"
                android:layout_height="77dp"
                android:background="@android:color/transparent"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/imageButton5"
                app:layout_constraintHorizontal_bias="0.301"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/clasesali"
                tools:ignore="SpeakableTextPresentCheck,DuplicateSpeakableTextCheck" />

            <ImageButton
                android:id="@+id/imageButton5"
                android:layout_width="80dp"
                android:layout_height="75dp"
                android:layout_marginEnd="8dp"
                android:background="@android:color/transparent"
                android:contentDescription="@string/todo"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/imageButton7"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.6"
                app:srcCompat="@drawable/blood"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/imageButton7"
                android:layout_width="67dp"
                android:layout_height="76dp"
                android:layout_marginEnd="88dp"
                android:background="@android:color/transparent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.444"
                app:srcCompat="@drawable/dietauno"
                tools:ignore="SpeakableTextPresentCheck"
                android:contentDescription="TODO" />

            <ImageButton
                android:id="@+id/imageButton8"
                android:layout_width="73dp"
                android:layout_height="77dp"
                android:background="@android:color/transparent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageButton7"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/pesonene"
                tools:ignore="SpeakableTextPresentCheck" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="329dp"
        android:layout_height="133dp"
        android:layout_marginTop="24dp"
        app:cardCornerRadius="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.529"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView5">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="316dp"
            android:layout_height="136dp">

            <TableLayout
                android:layout_width="312dp"
                android:layout_height="113dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.761"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.521">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <Button
                        android:id="@+id/button16"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_width="149dp"
                        android:layout_height="match_parent"
                        android:backgroundTint="#b00122"
                        android:text="@string/mi_perfil"
                        android:textColor="#FFFFFF"
                        app:cornerRadius="25dp" />

                    <Button
                        android:id="@+id/button18"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_width="149dp"
                        android:layout_height="match_parent"
                        android:backgroundTint="#b00122"
                        android:onClick="clases"
                        android:text="MIS CLASES"
                        android:textColor="#FFFFFF"
                        app:cornerRadius="25dp" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <Button
                        android:id="@+id/button19"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_width="163dp"
                        android:layout_height="match_parent"
                        android:backgroundTint="#b00122"
                        android:onClick="Insumos"
                        android:text="@string/mis_objetivos"
                        android:textColor="#FDF7F7"
                        app:cornerRadius="25dp" />

                    <Button
                        android:id="@+id/button20"
                        style="?android:attr/buttonBarButtonStyle"
                        android:layout_width="149dp"
                        android:layout_height="match_parent"
                        android:backgroundTint="#b00122"
                        android:onClick="calculoCalorias"
                        android:text="Metabolismo basal"
                        android:textColor="#F6F3F3"
                        app:cornerRadius="25dp" />
                </TableRow>

            </TableLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>
